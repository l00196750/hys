<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hys.db.dao.UserMapper">
    <select id="getUser" resultType="com.hys.db.model.DbUser">
        SELECT user_name, pwd FROM bfm_user LIMIT 1,1
    </select>
    
    <select id="getAllUser" resultType="com.hys.db.model.DbUser">
        SELECT user_name, pwd FROM bfm_user order by user_name
    </select>
    
    <select id="getAllUserWithRowBounds" resultType="com.hys.db.model.DbUser">
        SELECT user_name, pwd FROM bfm_user order by user_name
    </select>

    <insert id="insertTask" parameterType="java.lang.String">
        insert into lyq_tt(tid) values(#{tid})
    </insert>
</mapper>